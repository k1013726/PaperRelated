    "import pandas as pd\n",
    "from glob import glob\n",
    " \n",
    "files = glob('data/CoinText_*.csv')\n",
    " \n",
    "df = pd.concat(\n",
    "    (pd.read_csv(file, usecols=['CoinText','CoinDate'], dtype={ 'CoinText': str, 'CoinDate':str}) for file in files), ignore_index=True)\n",
    "df.to_csv('CoinText_final.csv')"
   ]
